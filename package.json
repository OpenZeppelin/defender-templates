{
    "name": "openzeppelin-ethdenver-solutions-boilerplate",
    "version": "1.0.0",
    "description": "Templates collection to empower your project with OpenZeppelin Defender and OpenZeppelin Contracts",
    "main": "index.js",
    "scripts": {
      "compile": "hardhat compile",
      "coverage": "env COVERAGE=true hardhat coverage",
      "lint:fix": "eslint ./ --ext js,jsx,ts,tsx --fix",
      "lint": "eslint ./ --ext js,jsx,ts,tsx && yarn run lint:sol",
      "lint:js": "prettier --loglevel warn --ignore-path .gitignore '**/*.{js,ts}' --check && eslint --ignore-path .gitignore .",
      "lint:js:fix": "prettier --loglevel warn --ignore-path .gitignore '**/*.{js,ts}' --write && eslint --ignore-path .gitignore . --fix",
      "lint:sol": "prettier --loglevel warn --ignore-path .gitignore '{contracts,test}/**/*.sol' --check && solhint '{contracts,test}/**/*.sol'",
      "lint:sol:fix": "prettier --loglevel warn --ignore-path .gitignore '{contracts,test}/**/*.sol' --write",
      "test": "hardhat test",
      "gas-report": "env ENABLE_GAS_REPORT=true yarn run test",
      "clean": "hardhat clean",
      "slither": "yarn run clean && slither . --detect reentrancy-eth,reentrancy-no-eth,reentrancy-unlimited-gas"
    },
    "contributors": [
      {
        "name": "OpenZeppelin Solutions",
        "email": "solutions@openzeppelin.com"
      }
    ],
    "license": "MIT",
    "devDependencies": {
      "@metamask/eth-sig-util": "^5.0.2",
      "@nomicfoundation/hardhat-chai-matchers": "^1.0.5",
      "@nomicfoundation/hardhat-network-helpers": "^1.0.7",
      "@nomiclabs/hardhat-truffle5": "^2.0.7",
      "@nomiclabs/hardhat-web3": "^2.0.0",
      "@openzeppelin/docs-utils": "^0.1.3",
      "@openzeppelin/hardhat-defender": "^1.8.1",
      "@openzeppelin/hardhat-upgrades": "^1.22.0",
      "@openzeppelin/test-helpers": "^0.5.16",
      "@openzeppelin/wizard": "^0.2.0",
      "@openzeppelin/wizard-cairo": "^0.5.0",
      "@safe-global/safe-core-sdk": "^3.2.1",
      "@safe-global/safe-ethers-lib": "^1.7.0",
      "@safe-global/safe-service-client": "^1.4.0",
      "@types/chai": "^4.3.1",
      "@types/mocha": "^9.1.1",
      "@types/node": "^18.11.18",
      "@types/node-fetch": "^2.6.2",
      "@types/yargs": "^17.0.19",
      "chai": "^4.3.7",
      "defender-serverless": "^1.0.3",
      "eslint": "^8.31.0",
      "eslint-config-prettier": "^8.5.0",
      "ethereumjs-util": "^7.1.5",
      "ethereumjs-wallet": "^1.0.2",
      "ethers": "^5.6.6",
      "glob": "^8.0.3",
      "graphlib": "^2.1.8",
      "hardhat": "^2.12.5",
      "hardhat-abi-exporter": "^2.10.1",
      "hardhat-exposed": "^0.3.0",
      "hardhat-gas-reporter": "^1.0.9",
      "hardhat-ignore-warnings": "^0.2.6",
      "keccak256": "^1.0.6",
      "merkletreejs": "^0.3.9",
      "prettier": "^2.8.2",
      "prettier-plugin-solidity": "^1.1.1",
      "serverless": "^3.20.0",
      "solhint": "^3.3.7",
      "solidity-coverage": "^0.8.2",
      "solidity-docgen": "^0.6.0-beta.34",
      "typechain": "^8.1.1",
      "web3": "^1.8.1",
      "yargs": "^17.0.0",
      "eslint-plugin-promise": "^6.1.1",
      "@typescript-eslint/parser": "^5.48.1",
      "@typechain/hardhat": "^6.1.5",
      "@typechain/ethers-v5": "^10.2.0",
      "@typechain/web3-v1": "^6.0.2",
      "hardhat-contract-sizer": "^2.6.1"
    },
    "dependencies": {
      "@openzeppelin/contracts": "^4.8.0",
      "@openzeppelin/contracts-upgradeable": "^4.8.0",
      "defender-admin-client": "^1.37.0",
      "defender-autotask-client": "^1.37.0",
      "defender-relay-client": "^1.37.0",
      "defender-sentinel-client": "^1.37.0"
    }
  }